        "src/page-market.html",
        "src/page-rings.html",
        "src/page-ring.html",
        "src/page-orders.html",
        "src/page-order.html",
        "src/page-fills.html",
        "src/page-wallet.html",
        "src/page-authorize.html",
        "src/page-convert.html",
        "src/page-transfer.html",
        "src/page-settings.html",
        "src/page-doc.html",
        "src/page-404.html",
        "src/page-dev.html",
        "src/page-unlockwallet.html",
        "src/page-generatewallet.html"
